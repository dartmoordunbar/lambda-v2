<section>
    <h1 id="chapter5">Appendix {{getappendixnumber()}}: Detailed results â€“ natural capital stocks</h1>
    <!---- TABLE A1 ---->
    {% set message = 'Table ' +  gettablenumber('new') + '.' %}
    {{label(message,'Estimated broad landcover classes in your chosen report area (hectares)')}}
      <table style="width:100%">
          <tr class="header">
            <th>Landcover</th>
            <th>Area</th>
          </tr>
          {% for item in table1['landcover'] %}
          <tr>
            <td>{{item.name}}</td>
            <td>{{item.amount | processNumber}}<span class="unit">ha</span></td>
          </tr>
          {% endfor %}
          <tr class="subheader">
            <td class="subheader">Total Area</td>
            <td class="subheader">{{table1['landcover_total_area'] | processNumber}}<span class="unit">ha</span></td>
          </tr>
      </table>
    {# <!---- TABLE A2 ----> #}
    {%if tableA2%}
    {% set message = 'Table ' +  gettablenumber('new') + '.' %}
    {{label(message,'Height of trees found in different landcover types')}}
    <table style="width:100%">
        <tr class="header">
          <th>Landcover</th>
          <th>Area</th>
          <th>Average Height</th>
        </tr>
        {% for item in tableA2 %}
        <tr>
          <td>{{item.name}}</td>
          <td>{{item.area | processNumber}}<span class="unit">ha</span></td>
          <td>{{item.avg_height | processNumber}}<span class="unit">m</span></td>
        </tr>
        {% endfor %}
    </table>
    {%endif%}
    {# Topography Table #}
    {%if table1 %}
    {% set message = 'Table ' +  gettablenumber('new') + '.' %}
    {{label(message,'Estimated topographical variation: elevation (metres above sea level) and slope (%)')}}
      <table style="width:100%">
        <tr class="header">
          <th>Topography</th>
          <th>Average</th>
        </tr>
        {% for item in table1['topography'] %}
        <tr>
          <td>{{item.name}}</td>
          <td class="twentypc">{{item.amount | processNumber}} <span class="unit">{{item.unit}}</span></td>
        </tr>
        {% endfor %}
      </table>
    {%endif%}
    {%if table1 %}
    {# Water and wetlands table #}
    {% set message = 'Table ' +  gettablenumber('new') + '.' %}
    {{label(message,'Estimated extent of water and wetlands')}}
      <table style="width:100%">
        <tr class="header">
          <th>Water and Wetlands</th>
          <th>Area</th>
        </tr>
        {% for item in table1['water'] %}
        {%if item %}
          <tr>
            <td>{{item.name}}</td>
            <td class="twentypc">{{item.amount | processNumber}}<span class="unit">ha</span></td>
          </tr>
        {%else%}
          <tr>
            <td colspan="2">No data</td>
          </tr>
        {%endif%}
        {% endfor %}
      </table>
      {%endif%}
    {# Soil Table #}
    {%if table1%}
    {% set message = 'Table ' +  gettablenumber('new') + '.' %}
    {{label(message,'Estimated extent of different soil types')}}
      <table style="width:100%">
        <tr class="header">
          <th>Soils</th>
          <th>Your Area</th>
        </tr>
        {% for item in table1['soil'] %}
        <tr>
          <td>{{item.name}}</td>
          <td class="twentypc">{{item.amount  | processNumber}}<span class="unit">ha</span></td>
        </tr>
        {% endfor %}
      </table> 
    {%endif%}
  </section>